{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "PnP": "Information"
    }
  },
  "extensions": {
    "queues": {
      "batchSize": 1,
      "maxDequeueCount": 3
    }
  }
}